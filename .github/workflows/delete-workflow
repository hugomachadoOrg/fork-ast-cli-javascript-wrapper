name: Javascript Wrapper Release

on:
  workflow_call:
    inputs:
      tag:
        description: 'Tag to delete'
        required: true
        type: string
        
        
permissions:
  id-token: write
  contents: write
  packages: write
  
  
jobs:          
  delete:
    runs-on: ubuntu-latest
    steps:
    
      - name: Print next tag version
        run: |
          echo "Tag ${{inputs.tag}} will be deleted!"          
